<%= simple_form_for [:workplace, @organization, @timetable].flatten do |f| %>
  <%= f.input :title %>
  <%= f.input :starts_on %>
  <%= f.input :ends_on %>
  <%= f.input :parity %>
  <%= f.input :first_week_parity, :as => :select %>

  <div class="buttons">
    <%= f.submit "Сохранить" %>
    <%= link_to "Отмена", :back %>
  </div>
<% end %>
